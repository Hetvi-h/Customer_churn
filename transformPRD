const features = metadata.feature_importance;

{features.map((feature, idx) => (
  <FeatureSection key={feature.name}>
    <FeatureHeader
      rank={idx + 1}
      name={feature.name}  // ANY feature!
      importance={feature.importance}
    />
    <FeatureDistribution
      feature={feature.name}
      data={featureData[feature.name]}  // Fetched from API
    />
  </FeatureSection>
))}
